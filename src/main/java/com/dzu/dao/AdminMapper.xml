<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dzu.dao.AdminMapper">
    <select id="queryAdminByName" resultType="com.dzu.pojo.Admin" parameterType="java.lang.String">
        select * from admin where username=#{username}
